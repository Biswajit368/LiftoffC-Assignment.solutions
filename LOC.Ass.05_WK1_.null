#include<stdio.h>

int main ()
{
  char ch;
  printf("Enter any character: \n");
  scanf("%c",&ch);
  switch(ch)
   {
     case 'a':
     case 'e':
     case 'i':
     case 'o':
     case 'u':
     case 'A':
     case 'E':
     case 'I':
     case 'O':
     case 'U':
       printf("It's a Vowel");
       break;
      default:
        printf("It's a Consonant");
    }
  return 0;
}